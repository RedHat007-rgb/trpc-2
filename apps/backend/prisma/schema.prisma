
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_SQL")
}


model User{
   id          String   @id @default(uuid()) 
  name String
  email String
  mobile String
}


model Todo{
   id          String   @id @default(uuid()) 
  title String
  description String
  done Boolean
}



